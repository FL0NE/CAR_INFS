        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="135dp" />

    <EditText
        android:id="@+id/num_Atemp"
        android:layout_width="50dp"
        android:layout_height="36dp"
        android:ems="10"
